{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  packageRules: [
    {
      description: "Changelog URL for Cloudflared",
      matchPackageNames: ["/cloudflared/"],
      changelogUrl: "https://github.com/cloudflare/cloudflared/blob/master/RELEASE_NOTES",
    },
    {
      description: "Changelog URL for cloudnative-pg",
      matchPackageNames: ["/cloudnative-pg/"],
      changelogUrl: "https://github.com/cloudnative-pg/cloudnative-pg/releases/latest",
    },
    {
      description: "Changelog URL for prompp",
      matchPackageNames: ["/prompp/"],
      changelogUrl: "https://github.com/deckhouse/prompp/blob/pp/CHANGELOG.md",
    },
    {
      description: "Changelog URL for Spegel",
      matchPackageNames: ["/spegel/"],
      changelogUrl: "https://github.com/spegel-org/spegel/blob/main/CHANGELOG.md",
    },
  ],
}
